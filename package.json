{
  "name": "nest-template",
  "version": "0.0.1",
  "description": "NestJs template using typescript",
  "author": "memoer",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist && yarn merge:gql",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "scripts/start-dev.sh",
    "start:debug": "scripts/start-debug.sh",
    "start:dist": "scripts/start-dist.sh",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test-e2e/jest-e2e.json",
    "merge:gql": "node scripts/gen-merged-gql.js",
    "gen:dto": "node scripts/gen-dto.js",
    "gen:module": "scripts/gen-module.sh",
    "typeorm": "./scripts/typeorm.sh"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ],
    "libs/**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^9.0.4",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@sentry/node": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "aws-sdk": "^2.988.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "dayjs": "^1.10.7",
    "graphql": "^15.5.3",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^12.0.0",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/merge": "^8.1.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
